{
  "name": "@horario-estudantil/ui",
  "version": "4.0.0-beta.0",
  "private": true,
  "scripts": {
    "dev": "npm run api:generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "api:generate": "npm run api:generate:gql",
    "api:generate:gql": "mkdir -p src/graphql/__generated__ && rm -r src/graphql/__generated__ && graphql-codegen && npm run api:generate:gql:fix",
    "api:generate:gql:fix": "node scripts/fixGraphql.js"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@mui/x-data-grid": "^5.17.14",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "next": "13.0.0",
    "next-auth": "^4.18.0",
    "next-pwa": "^5.6.0",
    "nprogress": "^0.2.0",
    "openid-client": "^5.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.40.0",
    "route-parser": "^0.0.5",
    "use-debounce": "^8.0.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/client-preset": "^1.2.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.11.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/route-parser": "^0.1.4",
    "babel-loader": "^9.0.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.0",
    "typescript": "4.8.4"
  }
}
